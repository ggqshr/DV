<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ggq.mapper.UserTableMapper">
    <select id="getUserByUserName" resultType="ggq.model.UserTable">
        select *
        from user_table
        where user_name = #{0}
    </select>
</mapper>